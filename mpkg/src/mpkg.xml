<?xml version="1.0" encoding="utf-8"?>
<mpkgconfig>
	<run_scripts>no</run_scripts>
	<sys_root>/root/development/sys_root/</sys_root>
	<sys_cache>/root/development/sys_cache/</sys_cache>
	<database_url>sqlite://var/log/mpkg/packages.db</database_url>
	<scripts_dir>/var/log/mpkg/scripts/</scripts_dir>
	<repository_list>
		<repository>ftp://localhost/mpkg-current/</repository>
	</repository_list>
</mpkgconfig>

