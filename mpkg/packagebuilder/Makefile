	$(COPY_FILE) --parents $(SOURCES) $(DIST) .tmp/packagebuilder1.0.0/ && $(COPY_FILE) --parents mainwindow.h .tmp/packagebuilder1.0.0/ && $(COPY_FILE) --parents main.cpp mainwindow.cpp .tmp/packagebuilder1.0.0/ && $(COPY_FILE) --parents package_builder.ui .tmp/packagebuilder1.0.0/ && (cd `dirname .tmp/packagebuilder1.0.0` && $(TAR) packagebuilder1.0.0.tar packagebuilder1.0.0 && $(COMPRESS) packagebuilder1.0.0.tar) && $(MOVE) `dirname .tmp/packagebuilder1.0.0`/packagebuilder1.0.0.tar.gz . && $(DEL_FILE) -r .tmp/packagebuilder1.0.0


